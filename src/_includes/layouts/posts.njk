---
layout: layouts/base.njk
templateClass: tmpl-home
---

<div class="card">
  <div class="content">
    {{ content | safe }}
    <ul>
      {%- for post in collections.posts -%}
        <li{% if page.url == post.url %} aria-current="page"{% endif %}>
        <a href='{{ post.url }}'>{{ post.data.title }}</a></li>
      {%- endfor -%}
    </ul>
  </div>
</div>
